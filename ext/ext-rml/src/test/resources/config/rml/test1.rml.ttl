@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .

@prefix : <http://dotwebstack.org/id/mapping/ext-rml/> .

:Test1 a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "" ;
    rml:referenceFormulation ql:JSONPath ;
    rml:iterator "$" ;
  ] ;
  rr:subjectMap [
    rml:reference "" ;
    rr:class :Test1 ;
  ] ;
.
