schema {
  query: Query
}

type Query {
  pand(
    identificatiecode: ID! @subject(prefix: "http://bag.basisregistraties.overheid.nl/bag/id/")
  ): Pand @source(backend: "local")
}

type Pand
    @shape(uri: "http://bag.basisregistraties.overheid.nl/def/bag/Pand",
        graph: "http://bag.basisregistraties.overheid.nl/bag/id/shapes/imbag") {
  identificatiecode: ID!
  status: String!
  oorspronkelijkBouwjaar: Int!
  beginGeldigheid: DateTime!
  eindGeldigheid: DateTime
}
