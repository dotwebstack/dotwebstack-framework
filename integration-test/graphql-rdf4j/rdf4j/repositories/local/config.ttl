@prefix sr: <http://www.openrdf.org/config/repository/sail#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#local> a rep:Repository;
  rep:repositoryID "local";
  rep:repositoryImpl [
      rep:repositoryType "openrdf:SailRepository";
